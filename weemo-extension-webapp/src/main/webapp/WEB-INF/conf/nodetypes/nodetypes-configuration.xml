<nodeTypes 
   xmlns:nt="http://www.jcp.org/jcr/nt/1.0" 
   xmlns:mix="http://www.jcp.org/jcr/mix/1.0" 
   xmlns:jcr="http://www.jcp.org/jcr/1.0" 
   xmlns:exo="http://www.exoplatform.com/jcr/exo/1.0">  

  <nodeType name="exo:videoCallProfile" isMixin="false" hasOrderableChildNodes="false" primaryItemName="">
    <supertypes>
      <supertype>nt:base</supertype>
    </supertypes>    
    <propertyDefinitions>
      <propertyDefinition name="exo:disableVideoCall" requiredType="Boolean" autoCreated="false" mandatory="true"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>
      <propertyDefinition name="exo:videoCallPermissions" requiredType="String" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>
      <propertyDefinition name="exo:weemoKey" requiredType="String" autoCreated="false" mandatory="true"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>     
      <propertyDefinition name="exo:authId" requiredType="String" autoCreated="false" mandatory="true"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>
      <propertyDefinition name="exo:authSecret" requiredType="String" autoCreated="false" mandatory="true"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>
      <propertyDefinition name="exo:passPhrase" requiredType="String" autoCreated="false" mandatory="true"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>
      <propertyDefinition name="exo:profileId" requiredType="String" autoCreated="false" mandatory="true"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>
      <propertyDefinition name="exo:domainId" requiredType="String" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>
    </propertyDefinitions>
    <childNodeDefinitions>
		<childNodeDefinition name="*" defaultPrimaryType="nt:file" autoCreated="false" mandatory="false" onParentVersion="COPY" protected="false" sameNameSiblings="false" >
			<requiredPrimaryTypes>
				<requiredPrimaryType>nt:file</requiredPrimaryType>
			</requiredPrimaryTypes>
		</childNodeDefinition>
	</childNodeDefinitions>   
  </nodeType> 
  
  
  <nodeType name="exo:videoCallCertificate" isMixin="true" hasOrderableChildNodes="false" primaryItemName="">
    <propertyDefinitions>
      <propertyDefinition name="exo:videoCallCertificateFileName" requiredType="String" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>    
    </propertyDefinitions>
  </nodeType>

</nodeTypes>
  